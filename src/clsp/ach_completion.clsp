(mod
  (
    TIMELOCK
    P2_PH
    my_amount
    fee
  )

  (include condition_codes.clib)

  (list
    (list ASSERT_SECONDS_RELATIVE TIMELOCK)
    (list ASSERT_MY_AMOUNT (+ my_amount fee))
    (list CREATE_COIN P2_PH my_amount)
    (list RESERVE_FEE fee)
    (list CREATE_COIN_ANNOUNCEMENT ())  ; for external fees
  )
)
